<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.itrack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ITrack">
        <activity android:name=".Pedometer"></activity>
        <activity android:name=".EnterUser" />
        <activity android:name=".GraphdataFragment" />
        <activity android:name=".MoodTracker" />
        <activity
            android:name=".grapDataFragmentTest"
            android:label="@string/title_activity_grap_data_fragment_test"
            android:theme="@style/Theme.ITrack.NoActionBar" />
        <activity android:name=".graphData" />
        <activity
            android:name=".Home"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".startInformation"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".LogIn" />
        <activity
            android:name=".welcomeWindow"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    </application>

</manifest>